fn fibonacci_, ([] (n {} num), (prev {} num), (beforePrev {} num)), ;
    (($ n) = 1) |-> ;
        $ beforePrev.
    , ;
        fibonacci_ (($ n) - 1), (($ prev) + ($ beforePrev)), ($ prev)..

fn fibonacci, ([] (n {} num)), ;
    fibonacci_ ($ n), 1, 0.

